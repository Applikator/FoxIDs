{
  "Settings": {
    "FoxIDsEndpoint": "https://foxidsxxxx.azurewebsites.net", // or custom domain "https://foxidsxxxx.com" or local development https://localhost:44330
    "Authority": "{foxids_endpoint}/{tenant_name}/master/{client_id}(login)/",
    "ClientId": "foxids_control_client",
    "Scope": "email foxids_control_api:foxids_tenant",
    "MasterScope": "foxids_control_api:foxids_master",
    "LoginCallBackPath": "{tenant_name}/authentication/login_callback",
    "LogoutCallBackPath": "{tenant_name}/authentication/logout_callback"
  }
}