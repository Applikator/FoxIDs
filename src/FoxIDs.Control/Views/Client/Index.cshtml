@model ClientViewModel
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>FoxIDs Control Client</title>
    <base href="/@Model.TenantName/" />
    <environment>
        <link href="~/css/bootstrap/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/app.css" rel="stylesheet" asp-append-version="true" />
    </environment>
</head>

<body>
    <app>Loading...</app>

    <div id="blazor-error-ui">
        An unhandled error has occurred.
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
    <environment>
        <script src="~/_content/Microsoft.AspNetCore.Components.WebAssembly.Authentication/AuthenticationService.js" asp-append-version="true"></script>
        <script src="~/_framework/blazor.webassembly.js" asp-append-version="true"></script>
    </environment>
</body>
</html>
